// Online C compiler to run C program online
#include <stdio.h>
swap(int x,int y)
{
    printf("value before swapping is X=%d and Y=%d \n",x,y);
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("after swapping Is X=%d and Y=%d",x,y);
}
int main() {
    int a,b;
    printf("enter value of a = ");
    scanf("%d",&a);
    printf("enter value of b =");
    scanf("%d",&b);
    swap(a,b);
}
