// Online C compiler to run C program online
#include <stdio.h>
void swap(int *x,int *y)
{
    int temp;
    temp=*x;
    *x=*y;
   *y=temp;
}
 main() {
    int a,b;
    printf("enter value of a = ");
    scanf("%d",&a);
    printf("enter value of b =");
    scanf("%d",&b);
    printf("before swapping \n");
    printf("a=%d,b=%d",a,b);
    swap(&a,&b);
    printf("\n after swapping \n");
    printf("a=%d,b=%d",a,b);
}
