// find area and volume of sphere 
#include<stdio.h>
main()
{
    int area,r, volume;
    const Pi=3.14;
    
    printf(" enter radious of sphere r= ");
    scanf("%d",&r);
    area=4*3.14*r*r;
    volume=(4/3)*3.14*r*r*r;
    printf(" surface area of sphere is %d", area);
    printf(" \n volume of sphere is %d", volume);
}
