// Online C compiler to run C program online
#include <stdio.h>
hello()
{
    printf("welcome to c\n");
}
areaofcircle(int r)
{   
    float area;
    area=3.14*r*r;
    printf("area of circle is %f ",area);
    
}
main()
{
    hello();
    int r;
    
    printf("enter radious of circle = ");
    scanf("%d",&r);
    areaofcircle(r);
     
}
