// Online C compiler to run C program online
#include <stdio.h>

add(int a,int b)
{
    printf("addition is %d \n ",a+b);
}
sub(int a,int b)
{
    printf("substraction is %d \n ",a-b);
}
mul(int a,int b)
{
    printf("multiple  is %d \n",a*b);
}
div(int a,int b)
{
    printf("divide is %d",a/b);
}

 main() {
    int a,b;
    printf("enter value of a = ");
    scanf("%d",&a);
     printf("enter value of b = ");
     scanf("%d",&b);
     
     add(a,b);
     sub(a,b);
     mul(a,b);
     div(a,b);
}
